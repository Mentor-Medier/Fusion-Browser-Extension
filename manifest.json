{
  "name": "Arc Fusion",
  "description": "Check fusion data",
  "version": "0.1",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html",
    "default_icon": "arcxp.png"
  },
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://corecomponents.arcpublishing.com/*",
        "http://localhost/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}